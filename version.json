{
  "version": "1.0.6",
  "publicReleaseRefSpec": ["^refs/heads/main$", "^refs/tags/v\\d+\\.\\d+"],
  "cloudBuild": {
    "setVersionVariables": true,
    "buildMetadata": ""
  },
  "nugetPackageVersion": {
    "semVer": 2,
    "precision": "build"
  },
  "release": {
    "tagName": "v{version}",
    "versionIncrement": "minor"
  }
}
